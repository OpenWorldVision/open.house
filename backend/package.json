{
    "name": "gambit-backend",
    "version": "0.1.0",
    "license": "MIT",
    "module": ".dist/esm/index.js",
    "main": ".dist/cjs/index.js",
    "private": true,
    "files": [
        ".dist"
    ],
    "sideEffects": false,
    "types": ".dist/types/index.d.ts",
    "scripts": {
        "start": "node .dist/cjs/server",
        "dev": "tsnd -r dotenv/config --respawn ./src/server.ts",
        "clean": "yarn g:tsc:clean",
        "build:project": "yarn g:tsc:project",
        "build:cjs": "yarn g:tsc:cjs",
        "build": "yarn build:cjs"
    },
    "dependencies": {
        "@aelea/core": "0.18.0",
        "@aelea/utils": "1.1.0",
        "@ethersproject/abi": "5.4.0",
        "@ethersproject/abstract-signer": "5.4.1",
        "@ethersproject/bignumber": "^5.4.1",
        "@ethersproject/bytes": "5.4.0",
        "@ethersproject/contracts": "^5.4.1",
        "@ethersproject/providers": "5.4.4",
        "@mikro-orm/core": "4.5.7",
        "@mikro-orm/mongo-highlighter": "^1.0.0",
        "@mikro-orm/mongodb": "4.5.7",
        "@mikro-orm/reflection": "^4.5.7",
        "@most/core": "1.6.1",
        "@most/disposable": "1.3.0",
        "@most/prelude": "1.8.0",
        "@most/scheduler": "1.3.0",
        "@most/types": "1.1.0",
        "concurrently": "^6.2.1",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "express-promise-router": "^4.1.0",
        "express-session": "^1.17.2",
        "gambit-middleware": "workspace:*",
        "puppeteer": "9.1.1",
        "ws": "^8.2.0"
    },
    "devDependencies": {
        "@types/express": "^4.17.13",
        "@types/express-serve-static-core": "^4.17.24",
        "@types/express-session": "^1.17.4",
        "@types/node": "15.6.1",
        "@types/ws": "^7.4.7",
        "html-to-image": "^1.7.0",
        "ts-node": "^10.2.1",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.3.5"
    },
    "mikro-orm": {
        "useTsNode": true,
        "configPaths": [
            "./app/mikro-orm.config.ts",
            "./dist/mikro-orm.config.js"
        ]
    }
