{
    "name": "gambit-backend",
    "version": "0.2.0",
    "license": "MIT",
    "module": ".dist/esm/index.js",
    "main": ".dist/cjs/index.js",
    "private": true,
    "files": [
        ".dist"
    ],
    "sideEffects": false,
    "types": ".dist/types/index.d.ts",
    "scripts": {
        "start": "node .dist/cjs/server",
        "dev": "tsnd -r dotenv/config --respawn ./src/server.ts",
        "clean": "yarn g:tsc:clean",
        "gql-typings": "zeus ../subgraph/schema.graphql ./src  --ts",
        "build:project": "yarn g:tsc:project",
        "build:cjs": "yarn g:tsc:cjs",
        "build": "yarn build:cjs"
    },
    "dependencies": {
        "@aelea/core": "1.1.0",
        "@ethersproject/abi": "5.5.0",
        "@ethersproject/abstract-signer": "5.5.0",
        "@ethersproject/bignumber": "^5.5.0",
        "@ethersproject/bytes": "5.5.0",
        "@ethersproject/contracts": "^5.5.0",
        "@ethersproject/providers": "5.5.0",
        "@mikro-orm/core": "4.5.9",
        "@mikro-orm/mongo-highlighter": "^1.0.0",
        "@mikro-orm/mongodb": "4.5.9",
        "@mikro-orm/reflection": "^4.5.9",
        "@most/core": "1.6.1",
        "@most/disposable": "1.3.0",
        "@most/prelude": "1.8.0",
        "@most/scheduler": "1.3.0",
        "@most/types": "1.1.0",
        "@types/isomorphic-fetch": "^0.0.35",
        "@urql/core": "^2.3.3",
        "concurrently": "^6.3.0",
        "cors": "^2.8.5",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "express-promise-router": "^4.1.0",
        "express-session": "^1.17.2",
        "gambit-middleware": "workspace:*",
        "graphql": "^15.6.1",
        "isomorphic-fetch": "^3.0.0",
        "ws": "^8.2.3"
    },
    "devDependencies": {
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/express-serve-static-core": "^4.17.24",
        "@types/express-session": "^1.17.4",
        "@types/node": "16.11.2",
        "@types/ws": "^8.2.0",
        "ts-node": "^10.3.1",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.4.4"
    },
    "mikro-orm": {
        "useTsNode": true,
        "configPaths": [
            "./app/mikro-orm.config.ts",
            "./dist/mikro-orm.config.js"
        ]
    }
}
