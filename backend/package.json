{
    "name": "gambit-backend",
    "version": "0.0.0",
    "license": "MIT",
    "module": ".dist/esm/index.js",
    "main": ".dist/cjs/index.js",
    "private": true,
    "files": [
        ".dist"
    ],
    "sideEffects": false,
    "types": ".dist/types/index.d.ts",
    "scripts": {
        "start": "node .dist/cjs/server",
        "dev": "yarn g:tsc:cjs && node -r dotenv/config .dist/cjs/server",
        "clean": "yarn g:tsc:clean",
        "build:project": "yarn g:tsc:project",
        "build:cjs": "yarn g:tsc:cjs",
        "build": "yarn g:parallel 'yarn build:cjs' 'yarn build:project'"
    },
    "dependencies": {
        "@aelea/utils": "1.1.0",
        "@ethersproject/abi": "5.2.0",
        "@ethersproject/abstract-signer": "5.2.0",
        "@ethersproject/bignumber": "^5.2.0",
        "@ethersproject/bytes": "5.2.0",
        "@ethersproject/contracts": "^5.2.0",
        "@ethersproject/providers": "5.2.0",
        "@mikro-orm/core": "4.5.5-dev.18",
        "@mikro-orm/mongo-highlighter": "^1.0.0",
        "@mikro-orm/mongodb": "4.5.5-dev.18",
        "@mikro-orm/reflection": "^4.5.5",
        "@most/core": "1.6.1",
        "@most/disposable": "1.3.0",
        "@most/prelude": "1.8.0",
        "@most/scheduler": "1.3.0",
        "@most/types": "1.1.0",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "express-promise-router": "^4.1.0",
        "express-session": "^1.17.2",
        "gambit-middleware": "workspace:*",
        "ws": "^7.4.6"
    },
    "devDependencies": {
        "@types/express": "^4.17.12",
        "@types/express-session": "^1.17.3",
        "@types/node": "^15.6.1",
        "@types/ws": "^7.4.4"
    },
    "mikro-orm": {
        "useTsNode": true,
        "configPaths": [
            "./app/mikro-orm.config.ts",
            "./dist/mikro-orm.config.js"
        ]
    }
}
